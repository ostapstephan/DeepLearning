/pagesave save def
sh 0 translate 90 rotate
%%EndPageSetup
iso1dict begin
gsave
llx lly 12 add translate
/v 0 store
/x0 x v get 3.147420 add sx cw mul add store
/y0 y v get bfs th add sub store
x0 y0 moveto
(#!/bin/python3.6) c n
(#Ostap Voynarovskiy) N
(#CGML HW1) N
(#Sept 12 2018) N
() p n
(import) K
( numpy as np) p n
(import) K
( tensorflow as tf) p n
(import) K
( matplotlib.pyplot as plt) p n
(import) K
( matplotlib.mlab as mlab) p n
() N
(from) K
( tqdm ) p
(import) K
( tqdm) p n
() N
(NUM_PHIS = 7) N
(BATCH_SIZE = 32) N
(NUM_BATCHES = 400) N
() N
(class) K
( Data\(object\):) p n
(    ) S
(def) K
( __init__\(self\):) p n
() N
(        num_samp = 50) N
(        sigma = .1) N
(        np.random.seed\(31415\)) N
() N
(        ) S
(# We're going to learn these paramters) c n
(        ) p
(# we ignore the next two lines cuz we don't plug them into the y functio) c n
(n) N
(        ) p
(# w = np.atleast_2d\([4, 3, 4, 2]\).transpose\(\)) c n
(        ) p
(# b = 2) c n
() p n
(        ) S
(#creates your training data) c n
(        self.index = np.arange\(num_samp\)) p n
(        self.x = np.random.uniform\(size=\(num_samp, 1\)\)          ) S
(# random uniform) c n
( distribution ) N
(        self.eps = sigma*np.random.normal\(size=\(num_samp,1\)\)) p n
(        self.y = np.sin\(self.x*2*np.pi\) + self.eps              ) S
(# sin wave plus ) c n
(noise) N
() p n
() N
(    ) S
(def) K
( get_batch\(self\):) p n
(        choices = np.random.choice\(self.index, size=BATCH_SIZE\)) N
(        ) S
(return) K
( self.x[choices].flatten\(\), self.y[choices].flatten\(\)) p n
() N
() N
(def) K
( f\(x\):) p n
(    ) S
(#these are the initializations of the things we will learn ) c n
(    ) p
(# weights should all be close ish) c n
(    w =     tf.get_variable\(') p
(w) str
(', [NUM_PHIS, 1], tf.float32,tf.random_normal_ini) p n
(tializer\(\)\) ) N
(    ) S
(# start at 0 cuz why not) c n
(    b =     tf.get_variable\(') p
(b) str
(', [], tf.float32, tf.zeros_initializer\(\)\) ) p n
(    ) S
(# mu is the x loc of the gaussian so we use a uniform distribution) c n
(    mu =    tf.get_variable\(') p
(mu) str
(', [NUM_PHIS, 1], tf.float32, tf.random_uniform_) p n
(initializer\(\)\) ) N
(    ) S
(# the sigmas are gonna be approx ) c n
(    sig =   tf.get_variable\(') p
(sig) str
(', [NUM_PHIS, 1], tf.float32,tf.random_normal_ini) p n
(tializer\(\)\)) N
(    phi = tf.exp\(-tf.pow\(\(x-mu\)/sig, 2\)\)) N
(    ) S
(return) K
( tf.squeeze\(tf.matmul\(tf.transpose\(w\), phi\) + b\)) p n
(    ) N
(    ) N
(x = tf.placeholder\(tf.float32, [BATCH_SIZE]\) ) S
(#NUM_PHIS) c n
(y = tf.placeholder\(tf.float32, [BATCH_SIZE]\)) p n
(y_hat = f\(x\)) N
() N
(loss = tf.reduce_mean\(tf.pow\(y-y_hat, 2\)/2\) ) S
(#loss funtion) c n
(optim = tf.train.GradientDescentOptimizer\(learning_rate=0.1\).minimize\(loss\) ) p
